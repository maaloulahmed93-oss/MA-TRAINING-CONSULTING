{
  "version": 2,
  "name": "matc-admin-panel-fallback",
  "buildCommand": "npm ci && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "nodeVersion": "18.x",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://matc-backend.onrender.com/api",
    "NODE_ENV": "production"
  }
}
