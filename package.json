{
  "name": "ma-training-consulting",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Votre partenaire stratégique pour la transformation digitale et le développement des compétences",
  "scripts": {
    "dev": "vite",
    "dev:main": "vite --port 5173",
    "dev:admin": "cd admin-panel && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:all": "concurrently \"npm run dev:main\" \"npm run dev:admin\" \"npm run dev:backend\" --names \"MAIN,ADMIN,API\" --prefix-colors \"cyan,magenta,green\"",
    "prebuild": "npm install",
    "build": "npx vite build --mode production",
    "build:admin": "cd admin-panel && npm run build",
    "build:all": "npm run build && npm run build:admin",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "serve -s dist",
    "start": "serve -s dist",
    "type-check": "tsc --noEmit",
    "install:backend": "cd backend && npm install",
    "setup": "npm install && npm run install:backend && cd admin-panel && npm install"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@emotion/is-prop-valid": "^1.4.0",
    "@heroicons/react": "^2.2.0",
    "@vitejs/plugin-react": "^4.3.1",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.344.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1",
    "serve": "^14.2.0",
    "swiper": "^11.2.10",
    "vite": "^7.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0"
  }
}
